@model List<Entity.UserEntity>

@{
    ViewData["Title"] = "Home Page";
}

<div>
    <table>

        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Tel</th>
            </tr>
        </thead>

        <tbody >
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.FirstName</td>
                    <td>@item.LastName</td>
                    <td>@item.Tel</td>
                </tr>
            }
        </tbody>

    </table>
</div>
